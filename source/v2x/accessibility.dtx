%\begin{macro}{\accessPopStack}
%Des Weiteren ist es n�tig Elemente vom Stack zu entfernen und abzuarbeiten.
%\paragraph{Parameter}
%|#1| Type |#2| ObjNum |#3| KidsField |#4| Title
%    \begin{macrocode}
\def\accessPopStack#1#2#3#4{%
  \ifnum \the\@stackdepth <1\relax%
     \global\let#1\empty%
     \global\let#2\empty%
     \global\let#3\empty%
     \global\let#4\empty%
  \else%
